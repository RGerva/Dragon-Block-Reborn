{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:saber_tooth_tiger",
            "is_spawnable": true,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:saber_tooth_tiger_wild": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 4,
                    "attack_interval": 15,
                    "within_radius": 16,
                    "entity_types": [
                        {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "donkey"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "mule"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "pig"
                                    }
                                ]
                            },
                            "max_dist": 16
                        },
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "player"
                                    },
                                    {
                                        "test": "is_sneaking",
                                        "subject": "other",
                                        "value": false
                                    }
                                ]
                            },
                            "max_dist": 8,
                            "must_see": true
                        }
                    ]
                },
                "minecraft:tameable": {
                    "probability": 0.33,
                    "tame_items": [
                        "porkchop",
                        "cooked_porkchop"
                    ],
                    "tame_event": {
                        "event": "dbe:on_tame"
                    }
                }
            },
            "dbe:saber_tooth_tiger_tamed": {
                "minecraft:health": {
                    "value": 300,
                    "max": 300
                },
                "minecraft:attack": {
                    "damage": 30
                },
                "minecraft:behavior.owner_hurt_by_target": {
                    "priority": 1
                },
                "minecraft:behavior.owner_hurt_target": {
                    "priority": 2
                },
                "minecraft:behavior.stay_while_sitting": {
                    "priority": 3
                },
                "minecraft:behavior.tempt": {
                    "priority": 5,
                    "speed_multiplier": 1.2,
                    "items": [
                        "chicken",
                        "cooked_chicken",
                        "beef",
                        "cooked_beef",
                        "muttonRaw",
                        "muttonCooked",
                        "porkchop",
                        "cooked_porkchop",
                        "rabbit",
                        "cooked_rabbit",
                        "rotten_flesh"
                    ]
                },
                "minecraft:behavior.follow_owner": {
                    "priority": 6,
                    "speed_multiplier": 1.0,
                    "start_distance": 10,
                    "stop_distance": 2
                },
                "minecraft:is_tamed": { },
                "minecraft:sittable": { }
            }
        },

        "components": {
            "minecraft:type_family": {
                "family": [
                    "mob",
                    "saber_tooth_tiger"
                ]
            },
            "minecraft:loot": {
                "table": "loot_tables/entities/saber_tooth_tiger.json"
            },
            "minecraft:health": {
                "value": 50,
                "max": 50
            },
            "minecraft:attack": {
                "damage": 5
            },
            "minecraft:collision_box": {
                "width": 1.2,
                "height": 1.9
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0,
                "generates_bubbles": false
            },
            "minecraft:navigation.walk": {
                "can_path_over_water": true,
                "avoid_water": true,
                "avoid_damage_blocks": true
            },
            "minecraft:movement": {
                "value": 0.28
            },
            "minecraft:behavior.hurt_by_target": {
                "priority": 4
            },
            "minecraft:behavior.melee_box_attack": {
                "priority": 4,
                "cooldown_time": 1.5,
                "speed_multiplier": 1.4
            },
            "minecraft:behavior.look_at_player": {
                "priority": 6,
                "look_distance": 5,
                "probability": 0.02
            },
            "minecraft:behavior.nap": {
                "priority": 8,
                "cooldown_min": 2,
                "cooldown_max": 7,
                "mob_detect_dist": 4,
                "mob_detect_height": 2,
                "can_nap_filters": {
                    "all_of": [
                        { "test": "in_water", "subject": "self", "operator": "==", "value": false },
                        { "test": "on_ground", "subject": "self", "operator": "==", "value": true }
                    ]
                },
                "wake_mob_exceptions": {
                    "any_of": [
                        { "test": "is_family", "subject": "other", "operator": "==", "value": "saber_tooth_tiger" },
                        { "test": "is_sneaking", "subject": "other", "operator": "==", "value": true }
                    ]
                }
            },
            "minecraft:behavior.random_stroll": {
                "priority": 8,
                "speed_multiplier": 1
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self"
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "on_damage": {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "has_damage",
                                        "value": "fall"
                                    },
                                    {
                                        "test": "is_owner",
                                        "subject": "other"
                                    }
                                ]
                            }
                        },
                        "deals_damage": "no"
                    },
                    {
                        "on_damage": {
                            "filters": {
                                "test": "has_component",
                                "value": "minecraft:is_tamed"
                            }
                        },
                        "damage_multiplier": 0.1
                    }
                ]
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:despawn": {
                "despawn_from_distance": { }
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 128,
                    "max_dropped_ticks": 20,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:movement.basic": { },
            "minecraft:physics": { },
            "minecraft:nameable": { },
            "minecraft:is_hidden_when_invisible": { },
            "minecraft:jump.static": { },
            "minecraft:can_climb": { }
        },

        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [ "dbe:saber_tooth_tiger_wild" ]
                }
            },
            "dbe:on_tame": {
                "remove": {
                    "component_groups": [ "dbe:saber_tooth_tiger_wild" ]
                },
                "add": {
                    "component_groups": [ "dbe:saber_tooth_tiger_tamed" ]
                }
            }
        }
    }
}