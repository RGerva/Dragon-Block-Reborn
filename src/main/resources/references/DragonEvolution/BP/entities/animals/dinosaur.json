{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:dinosaur",
            "is_spawnable": true,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:orange_dinosaur": {
                "minecraft:variant": {
                    "value": 0
                }
            },
            "dbe:brown_dinosaur": {
                "minecraft:variant": {
                    "value": 1
                }
            },
            "dbe:yellow_dinosaur": {
                "minecraft:variant": {
                    "value": 2
                }
            },
            "dbe:dinosaur_wild": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 4,
                    "within_radius": 16,
                    "entity_types": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "animal"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "dinosaur"
                                    }
                                ]
                            },
                            "max_dist": 16
                        },
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "player"
                                    },
                                    {
                                        "test": "is_sneaking",
                                        "subject": "other",
                                        "value": false
                                    }
                                ]
                            },
                            "max_dist": 8,
                            "must_see": true
                        }
                    ]
                },
                "minecraft:tameable": {
                    "probability": 0.33,
                    "tame_items": [
                        "chicken",
                        "cooked_chicken",
                        "beef",
                        "cooked_beef",
                        "muttonRaw",
                        "muttonCooked",
                        "porkchop",
                        "cooked_porkchop",
                        "rabbit",
                        "cooked_rabbit",
                        "rotten_flesh"
                    ],
                    "tame_event": {
                        "event": "dbe:on_tame"
                    }
                }
            },
            "dbe:dinosaur_tamed": {
                "minecraft:health": {
                    "value": 900,
                    "max": 900
                },
                "minecraft:attack": {
                    "damage": 900
                },
                "minecraft:behavior.owner_hurt_by_target": {
                    "priority": 1
                },
                "minecraft:behavior.owner_hurt_target": {
                    "priority": 2
                },
                "minecraft:behavior.stay_while_sitting": {
                    "priority": 3
                },
                "minecraft:behavior.tempt": {
                    "priority": 5,
                    "speed_multiplier": 1.2,
                    "items": [
                        "chicken",
                        "cooked_chicken",
                        "beef",
                        "cooked_beef",
                        "muttonRaw",
                        "muttonCooked",
                        "porkchop",
                        "cooked_porkchop",
                        "rabbit",
                        "cooked_rabbit",
                        "rotten_flesh"
                    ]
                },
                "minecraft:behavior.follow_owner": {
                    "priority": 6,
                    "speed_multiplier": 1.0,
                    "start_distance": 10,
                    "stop_distance": 2
                },
                "minecraft:rideable": {
                    "seat_count": 1,
                    "crouching_skip_interact": true,
                    "family_types": [
                        "player"
                    ],
                    "interact_text": "action.interact.dinosaur",
                    "seats": {
                        "position": [ 0, 2.1, 0 ]
                    }
                },
                "minecraft:inventory": {
                    "inventory_size": 16,
                    "container_type": "horse"
                },
                "minecraft:equippable": {
                    "slots": [
                        {
                            "slot": 0,
                            "item": "saddle",
                            "accepted_items": [ "saddle" ],
                            "on_equip": {
                                "event": "dbe:dinosaur_saddled"
                            },
                            "on_unequip": {
                                "event": "dbe:dinosaur_unsaddled"
                            }
                        }
                    ]
                },
                "minecraft:is_tamed": { },
                "minecraft:sittable": { }
            },
            "dbe:dinosaur_saddled": {
                "minecraft:is_saddled": { },
                "minecraft:is_chested": { },
                "minecraft:input_ground_controlled": { },
                "minecraft:behavior.player_ride_tamed": { }
            }
        },

        "components": {
            "minecraft:type_family": {
                "family": [
                    "mob",
                    "dinosaur"
                ]
            },
            "minecraft:loot": {
                "table": "loot_tables/entities/dinosaur.json"
            },
            "minecraft:health": {
                "value": 9999999,
                "max": 9999999
            },
            "minecraft:attack": {
                "damage": 1
            },
            "minecraft:scale": {
                "value": 1.5
            },
            "minecraft:collision_box": {
                "width": 1.5,
                "height": 3
            },
            "minecraft:custom_hit_test": {
                "hitboxes": [
                    {
                        "width": 2,
                        "height": 1.5,
                        "pivot": [ 0, 2.25, -2.5 ]
                    },
                    {
                        "width": 2.75,
                        "height": 3,
                        "pivot": [ 0, 1.5, 0 ]
                    },
                    {
                        "width": 2,
                        "height": 1.5,
                        "pivot": [ 0, 2.25, 2.5 ]
                    }
                ]
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0,
                "generates_bubbles": false
            },
            "minecraft:navigation.walk": {
                "can_path_over_water": true,
                "avoid_water": true,
                "avoid_damage_blocks": true
            },
            "minecraft:movement": {
                "value": 0.22
            },
            "minecraft:behavior.hurt_by_target": {
                "priority": 4
            },
            "minecraft:behavior.delayed_attack": {
                "priority": 4,
                "cooldown_time": 1.5,
                "attack_duration": 0.5
            },
            "minecraft:behavior.random_stroll": {
                "priority": 8,
                "speed_multiplier": 1
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self"
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "on_damage": {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "has_damage",
                                        "value": "fall"
                                    },
                                    {
                                        "test": "is_owner",
                                        "subject": "other"
                                    }
                                ]
                            }
                        },
                        "deals_damage": "no"
                    },
                    {
                        "on_damage": {
                            "filters": {
                                "test": "has_component",
                                "value": "minecraft:is_tamed"
                            }
                        },
                        "damage_multiplier": 0.1
                    }
                ]
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:despawn": {
                "despawn_from_distance": { }
            },
            "minecraft:variable_max_auto_step": {
                "base_value": 1,
                "controlled_value": 1,
                "jump_prevented_value": 1
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 128,
                    "max_dropped_ticks": 20,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:movement.basic": { },
            "minecraft:physics": { },
            "minecraft:nameable": { },
            "minecraft:is_hidden_when_invisible": { },
            "minecraft:jump.static": { },
            "minecraft:can_climb": { }
        },

        "events": {
            "minecraft:entity_spawned": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "dbe:dinosaur_wild"
                            ]
                        }
                    },
                    {
                        "randomize": [
                            {
                                "add": {
                                    "component_groups": [
                                        "dbe:orange_dinosaur"
                                    ]
                                }
                            },
                            {
                                "add": {
                                    "component_groups": [
                                        "dbe:brown_dinosaur"
                                    ]
                                }
                            },
                            {
                                "add": {
                                    "component_groups": [
                                        "dbe:yellow_dinosaur"
                                    ]
                                }
                            }
                        ]
                    }
                ]
            },
            "dbe:on_tame": {
                "remove": {
                    "component_groups": [
                        "dbe:dinosaur_wild"
                    ]
                },
                "add": {
                    "component_groups": [
                        "dbe:dinosaur_tamed"
                    ]
                }
            },
            "dbe:dinosaur_saddled": {
                "add": {
                    "component_groups": [
                        "dbe:dinosaur_saddled"
                    ]
                }
            },
            "dbe:dinosaur_unsaddled": {
                "remove": {
                    "component_groups": [
                        "dbe:dinosaur_saddled"
                    ]
                }
            }
        }
    }
}