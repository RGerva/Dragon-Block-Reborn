{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:namek_frog",
            "is_spawnable": true,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:green_frog": {
                "minecraft:variant": {
                    "value": 0
                }
            },
            "dbe:pink_frog": {
                "minecraft:variant": {
                    "value": 1
                }
            },
            "dbe:yellow_frog": {
                "minecraft:variant": {
                    "value": 2
                }
            },
            "dbe:namek_frog_wild": {
                "minecraft:tameable": {
                    "probability": 0.33,
                    "tame_items": [
                        "dbe:medical_moss_seeds"
                    ],
                    "tame_event": {
                        "event": "dbe:on_tame"
                    }
                }
            },
            "dbe:namek_frog_tame": {
                "minecraft:health": {
                    "value": 15,
                    "max": 15
                },
                "minecraft:behavior.follow_owner": {
                    "priority": 6,
                    "speed_multiplier": 1.0,
                    "start_distance": 10,
                    "stop_distance": 2
                },
                "minecraft:behavior.breed": {
                    "priority": 2,
                    "speed_multiplier": 1.0
                },
                "minecraft:is_tamed": { },
                "minecraft:sittable": { }
            }
        },
        "components": {
            "minecraft:type_family": {
                "family": [
                    "frog",
                    "mob"
                ]
            },
            "minecraft:collision_box": {
                "width": 0.5,
                "height": 0.55
            },
            "minecraft:experience_reward": {
                "on_bred": "Math.Random(1,7)",
                "on_death": "q.last_hit_by_player ? Math.Random(1,3) : 0"
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0,
                "breathes_water": true,
                "breathes_air": true,
                "generates_bubbles": false
            },
            "minecraft:health": {
                "value": 10
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "fall",
                    "deals_damage": "yes",
                    "damage_modifier": -5
                }
            },
            "minecraft:navigation.generic": {
                "is_amphibious": true,
                "can_path_over_water": true,
                "can_swim": true,
                "can_walk": true,
                "can_sink": false,
                "avoid_damage_blocks": true
            },
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:underwater_movement": {
                "value": 0.15
            },
            "minecraft:despawn": {
                "despawn_from_distance": { }
            },
            "minecraft:breedable": {
                "require_tame": true,
                "breed_items": [ "dbe:medical_moss_seeds" ]
            },
            "minecraft:behavior.panic": {
                "priority": 1,
                "speed_multiplier": 2.0
            },
            "minecraft:behavior.breed": {
                "priority": 4
            },
            "minecraft:behavior.tempt": {
                "priority": 5,
                "speed_multiplier": 1.25,
                "can_tempt_vertically": true,
                "items": [ "dbe:medical_moss_seeds" ]
            },
            "minecraft:behavior.move_to_land": {
                "priority": 6,
                "search_range": 30,
                "search_height": 8,
                "search_count": 80,
                "goal_radius": 2
            },
            "minecraft:behavior.croak": {
                "priority": 9,
                "interval": [ 10, 20 ],
                "duration": 4.5,
                "filters": {
                    "all_of": [
                        {
                            "test": "in_water",
                            "value": false
                        },
                        {
                            "test": "in_lava",
                            "value": false
                        }
                    ]
                }
            },
            "minecraft:behavior.jump_to_block": {
                "priority": 10,
                "search_width": 8,
                "search_height": 4,
                "minimum_path_length": 2,
                "minimum_distance": 1,
                "scale_factor": 0.6,
                "max_velocity": 1,
                "cooldown_range": [ 5, 7 ],
                "preferred_blocks": [
                    "minecraft:waterlily",
                    "minecraft:big_dripleaf"
                ],
                "preferred_blocks_chance": 0.5,
                "forbidden_blocks": [
                    "minecraft:water"
                ]
            },
            "minecraft:behavior.random_stroll": {
                "priority": 11
            },
            "minecraft:behavior.look_at_player": {
                "priority": 12,
                "target_distance": 6.0,
                "probability": 0.02
            },
            "minecraft:is_hidden_when_invisible": { },
            "minecraft:jump.static": { },
            "minecraft:leashable": { },
            "minecraft:movement.amphibious": { },
            "minecraft:nameable": { },
            "minecraft:physics": { },
            "minecraft:pushable": { }
        },
        "events": {
            "minecraft:entity_spawned": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "dbe:namek_frog_wild"
                            ]
                        }
                    },
                    {
                        "randomize": [
                            {
                                "weight": 1,
                                "add": {
                                    "component_groups": [
                                        "dbe:green_frog"
                                    ]
                                }
                            },
                            {
                                "weight": 1,
                                "add": {
                                    "component_groups": [
                                        "dbe:pink_frog"
                                    ]
                                }
                            },
                            {
                                "weight": 1,
                                "add": {
                                    "component_groups": [
                                        "dbe:yellow_frog"
                                    ]
                                }
                            }
                        ]
                    }
                ]
            },
            "dbe:on_tame": {
                "remove": {
                    "component_groups": [
                        "dbe:namek_frog_wild"
                    ]
                },
                "add": {
                    "component_groups": [
                        "dbe:namek_frog_tame"
                    ]
                }
            }
        }
    }
}