{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:deer",
            "spawn_category": "creature",
            "is_spawnable": true,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:deer_baby": {
                "minecraft:scale": {
                    "value": 0.5
                },
                "minecraft:ageable": {
                    "duration": 1200,
                    "feed_items": [
                        "dbe:dirtstone_tall_grass",
                        "wheat"
                    ],
                    "grow_up": {
                        "event": "dbe:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.follow_parent": {
                    "priority": 6,
                    "speed_multiplier": 1.1
                },
                "minecraft:is_baby": { }
            },
            "dbe:deer_adult": {
                "minecraft:loot": {
                    "table": "loot_tables/entities/deer.json"
                },
                "minecraft:experience_reward": {
                    "on_bred": "Math.Random(1,7)",
                    "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
                },
                "minecraft:behavior.breed": {
                    "priority": 3,
                    "speed_multiplier": 1.0
                },
                "minecraft:breedable": {
                    "require_tame": false,
                    "breed_items": [
                        "dbe:dirtstone_tall_grass",
                        "wheat"
                    ],
                    "breeds_with": {
                        "mate_type": "dbe:deer",
                        "baby_type": "dbe:deer",
                        "breed_event": {
                            "event": "dbe:entity_born",
                            "target": "baby"
                        }
                    }
                }
            }
        },

        "components": {
            "minecraft:type_family": {
                "family": [
                    "deer",
                    "mob"
                ]
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0
            },
            "minecraft:navigation.walk": {
                "can_path_over_water": true,
                "avoid_water": true,
                "avoid_damage_blocks": true
            },
            "minecraft:collision_box": {
                "width": 0.9,
                "height": 1.7
            },
            "minecraft:health": {
                "value": 9999999,
                "max": 9999999
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:movement": {
                "value": 0.4
            },
            "minecraft:behavior.float": {
                "priority": 0
            },
            "minecraft:behavior.panic": {
                "priority": 1,
                "speed_multiplier": 1.25
            },
            "minecraft:behavior.mount_pathing": {
                "priority": 2,
                "speed_multiplier": 1.5,
                "target_dist": 0.0,
                "track_target": true
            },
            "minecraft:behavior.breed": {
                "priority": 3,
                "speed_multiplier": 1
            },
            "minecraft:behavior.tempt": {
                "priority": 4,
                "speed_multiplier": 1.25,
                "items": [
                    "dbe:dirtstone_tall_grass",
                    "wheat"
                ]
            },
            "minecraft:behavior.follow_parent": {
                "priority": 5,
                "speed_multiplier": 1.1
            },
            "minecraft:behavior.avoid_mob_type": {
                "priority": 6,
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "mob"
                                },
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "player"
                                }
                            ]
                        },
                        "max_dist": 16,
                        "walk_speed_multiplier": 1.4,
                        "sprint_speed_multiplier": 1.6
                    }
                ]
            },
            "minecraft:behavior.random_stroll": {
                "priority": 7,
                "speed_multiplier": 0.8
            },
            "minecraft:behavior.look_at_player": {
                "priority": 8,
                "look_distance": 6.0,
                "probability": 0.02
            },
            "minecraft:behavior.random_look_around": {
                "priority": 9
            },
            "minecraft:rideable": {
                "seat_count": 1,
                "family_types": [
                    "zombie"
                ],
                "seats": {
                    "position": [ 0, 1.1, -0.3 ]
                }
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:despawn": {
                "despawn_from_distance": { }
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 128,
                    "max_dropped_ticks": 20,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:balloonable": { },
            "minecraft:can_climb": { },
            "minecraft:jump.static": { },
            "minecraft:is_hidden_when_invisible": { },
            "minecraft:movement.basic": { },
            "minecraft:nameable": { },
            "minecraft:physics": { }
        },

        "events": {
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "weight": 95,
                        "trigger": "dbe:spawn_adult"
                    },
                    {
                        "weight": 5,
                        "add": {
                            "component_groups": [
                                "dbe:deer_baby"
                            ]
                        }
                    }
                ]
            },
            "dbe:entity_born": {
                "add": {
                    "component_groups": [
                        "dbe:deer_baby"
                    ]
                }
            },
            "dbe:entity_transformed": {
                "remove": {
                },
                "add": {
                    "component_groups": [
                        "dbe:deer_adult"
                    ]
                }
            },
            "dbe:ageable_grow_up": {
                "remove": {
                    "component_groups": [
                        "dbe:deer_baby"
                    ]
                },
                "add": {
                    "component_groups": [
                        "dbe:deer_adult"
                    ]
                }
            },
            "dbe:spawn_adult": {
                "add": {
                    "component_groups": [
                        "dbe:deer_adult"
                    ]
                }
            }
        }
    }
}