{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:pirate_robot_bullet",
            "runtime_identifier": "minecraft:snowball",
            "is_spawnable": false,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:despawn": {
                "minecraft:instant_despawn": { }
            }
        },

        "components": {
            "minecraft:collision_box": {
                "width": 0.2,
                "height": 0.1
            },
            "minecraft:timer": {
                "time": 5,
                "looping": false,
                "time_down_event": {
                    "event": "dbe:despawn"
                }
            },
            "minecraft:projectile": {
                "on_hit": {
                    "impact_damage": {
                        "damage": 25,
                        "knockback": true,
                        "should_bounce": true
                    }
                },
                "hit_sound": "combat.small_hit",
                "power": 3.8,
                "gravity": 0,
                "liquid_inertia": 0,
                "uncertainty_base": 1,
                "uncertainty_multiplier": 0,
                "anchor": 0,
                "should_bounce": true,
                "offset": [ 1.4, 1.5, 1.5 ]
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 128,
                    "max_dropped_ticks": 20,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:physics": { }
        },

        "events": {
            "dbe:despawn": {
                "add": {
                    "component_groups": [
                        "dbe:despawn"
                    ]
                }
            }
        }
    }
}