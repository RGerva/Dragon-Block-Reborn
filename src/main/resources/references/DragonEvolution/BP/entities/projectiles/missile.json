{
    "format_version": "1.21.90",
    "minecraft:entity": {
        "description": {
            "identifier": "dbe:missile",
            "runtime_identifier": "minecraft:snowball",
            "is_spawnable": false,
            "is_summonable": true
        },

        "component_groups": {
            "dbe:exploding": {
                "minecraft:explode": {
                    "fuse_length": 0,
                    "fuse_lit": true,
                    "power": 2,
                    "destroy_affected_by_griefing": true
                }
            },

            "dbe:despawn": {
                "minecraft:instant_despawn": { }
            }
        },

        "components": {
            "minecraft:type_family": {
                "family": [ "explode" ]
            },
            "minecraft:collision_box": {
                "width": 0.64,
                "height": 0.64
            },
            "minecraft:timer": {
                "time": 5,
                "looping": false,
                "time_down_event": {
                    "event": "dbe:explode"
                }
            },
            "minecraft:projectile": {
                "on_hit": {
                    "definition_event": {
                        "affect_projectile": true,
                        "event_trigger": {
                            "event": "dbe:explode",
                            "target": "self"
                        }
                    }
                },
                "power": 1.8,
                "gravity": 0.09,
                "inertia": 1,
                "angle_offset": -85,
                "anchor": 1
            },
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 128,
                    "max_dropped_ticks": 20,
                    "use_motion_prediction_hints": true
                }
            },
            "minecraft:physics": { }
        },

        "events": {
            "dbe:explode": {
                "add": {
                    "component_groups": [
                        "dbe:exploding"
                    ]
                }
            },
            "dbe:despawn": {
                "add": {
                    "component_groups": [
                        "dbe:despawn"
                    ]
                }
            }
        }
    }
}