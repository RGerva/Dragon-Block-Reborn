{
    "format_version": "1.16.0",
    "minecraft:feature_rules": {
        "description": {
            "identifier": "dbe:dirtstone_surface_rocks_feature",
            "places_feature": "dbe:scatter_dirtstone_rock_feature"
        },

        "conditions": {
            "placement_pass": "first_pass",
            "minecraft:biome_filter": {
                "test": "has_biome_tag",
                "operator": "==",
                "value": "dirtstone"
            }
        },
        "distribution": {
            "iterations": 1,
            "scatter_chance": 11,
            "x": {
                "distribution": "uniform",
                "extent": [ 0, 16 ]
            },
            "y": "q.heightmap(v.worldx, v.worldz)",
            "z": {
                "distribution": "uniform",
                "extent": [ 0, 16 ]
            }
        }
    }
}