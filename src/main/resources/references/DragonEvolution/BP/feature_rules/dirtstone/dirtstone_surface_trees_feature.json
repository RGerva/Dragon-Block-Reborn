{
    "format_version": "1.16.0",
    "minecraft:feature_rules": {
        "description": {
            "identifier": "dbe:dirtstone_surface_trees_feature",
            "places_feature": "dbe:scatter_dirtstone_tree_feature"
        },

        "conditions": {
            "placement_pass": "surface_pass",
            "minecraft:biome_filter": {
                "test": "has_biome_tag",
                "operator": "==",
                "value": "dirtstone"
            }
        },
        "distribution": {
            "iterations": 1,
            "coordinate_eval_order": "zxy",
            "x": 0,
            "y": "query.heightmap(variable.worldx, variable.worldz)",
            "z": 0
        }
    }
}