{
    "format_version": "1.21.90",
    "minecraft:block": {
        "description": {
            "identifier": "dbe:hyperbolic_time_chamber_door",
            "states": {
                "dbe:has_lock": [ false, true ]
            },
            "traits": {
                "minecraft:placement_direction": {
                    "enabled_states": [ "minecraft:cardinal_direction" ]
                }
            }
        },

        "components": {
            "minecraft:map_color": "#90552b",
            "minecraft:destruction_particles": {
                "texture": "hyperbolic_time_chamber_door"
            },
            "minecraft:light_dampening": 0,
            "minecraft:collision_box": false,
            "minecraft:selection_box": false,
            "minecraft:geometry": "geometry.hyperbolic_time_chamber_door",
            "minecraft:material_instances": {
                "*": {
                    "texture": "hyperbolic_time_chamber_door",
                    "render_method": "opaque",
                    "ambient_occlusion": false
                }
            },
            "minecraft:liquid_detection": {
                "detection_rules": [
                    {
                        "can_contain_liquid": true
                    }
                ]
            }
        },

        "permutations": [
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'west'",
                "components": {
                    "minecraft:transformation": {
                        "rotation": [ 0, -90, 0 ]
                    }
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'south'",
                "components": {
                    "minecraft:transformation": {
                        "rotation": [ 0, 0, 0 ]
                    }
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'east'",
                "components": {
                    "minecraft:transformation": {
                        "rotation": [ 0, 90, 0 ]
                    }
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'north'",
                "components": {
                    "minecraft:transformation": {
                        "rotation": [ 0, 180, 0 ]
                    }
                }
            },
            {
                "condition": "q.block_state('dbe:has_lock')",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "hyperbolic_time_chamber_door_lock_block",
                            "render_method": "opaque",
                            "ambient_occlusion": false
                        }
                    }
                }
            }
        ]
    }
}