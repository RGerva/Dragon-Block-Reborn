{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.deer.general": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"idle_controller",
						"move_controller",
						"look_at_target"
					]
				}
			}
		},
		"controller.animation.deer.idle": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [ "idle" ]
				}
			}
		},
		"controller.animation.deer.move": {
			"states": {
				"default": {
					"blend_transition": 0.3,
					"transitions": [
						{
							"walking": "q.ground_speed"
						}
					]
				},
				"walking": {
					"blend_transition": 0.3,
					"animations": [ "walk" ],
					"transitions": [
						{
							"default": "!q.ground_speed"
						}
					]
				}
			}
		}
	}
}