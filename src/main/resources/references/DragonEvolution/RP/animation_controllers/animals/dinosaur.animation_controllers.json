{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.dinosaur.general": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"attack_controller",
						"idle_controller",
						"look_at_target",
						"setup",
						"move_controller"
					]
				}
			}
		},
		"controller.animation.dinosaur.attack": {
			"states": {
				"default": {
					"transitions": [
						{
							"attacking": "v.attack_time"
						}
					]
				},
				"attacking": {
					"animations": [ "attack" ],
					"transitions": [
						{
							"default": "q.any_animation_finished"
						}
					]
				}
			}
		},
		"controller.animation.dinosaur.idle": {
			"states": {
				"default": {
					"blend_transition": 0.2,
					"transitions": [
						{
							"idling": "!q.ground_speed"
						}
					]
				},
				"idling": {
					"blend_transition": 0.2,
					"animations": [ "idle" ],
					"transitions": [
						{
							"default": "q.ground_speed"
						}
					]
				}
			}
		},
		"controller.animation.dinosaur.move": {
			"states": {
				"default": {
					"blend_transition": 0.3,
					"transitions": [
						{
							"walking": "q.ground_speed"
						}
					]
				},
				"walking": {
					"blend_transition": 0.3,
					"animations": [ "walk" ],
					"transitions": [
						{
							"default": "!q.ground_speed"
						}
					]
				}
			}
		}
	}
}