{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "dbe:giant_fish",
            "materials": {
                "default": "entity_alphatest"
            },
            "textures": {
                "default": "textures/entity/animals/giant_fish/giant_fish"
            },
            "geometry": {
                "default": "geometry.giant_fish"
            },
            "spawn_egg": {
                "base_color": "#6bbecf",
                "overlay_color": "#2a419d"
            },
            "scripts": {
                "pre_animation": [
                    "v.zrot = !q.is_in_water ? math.cos((q.time_stamp + q.frame_alpha) * 14.32) * 90 : 0.0;"
                ],
                "animate": [
                    "general",
                    "attack_controller"
                ]
            },
            "animations": {
                "general": "controller.animation.giant_fish.general",
                "attack": "animation.giant_fish.attack",
                "attack_controller": "controller.animation.giant_fish.attack",
                "flop": "animation.giant_fish.flop",
                "idle": "animation.giant_fish.idle",
                "swim": "animation.giant_fish.swim"
            },
            "render_controllers": [
                "controller.render.default"
            ]
        }
    }
}