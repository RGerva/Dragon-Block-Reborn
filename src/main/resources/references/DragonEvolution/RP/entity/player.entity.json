					"controller.render.player.majin.first_person": "v.is_first_person && !q.is_spectator && v.race == 'majin'"
				},
				{
					"controller.render.player.majin.third_person": "!v.is_first_person && !q.is_spectator && !v.map_face_icon && v.race == 'majin'"
				},
				// bio android
				{
					"controller.render.player.bio_android.eyes": "!v.is_first_person && !q.is_spectator && v.race == 'bio_android'"
				},
				{
					"controller.render.player.bio_android.first_person": "v.is_first_person && !q.is_spectator && v.race == 'bio_android'"
				},
				{
					"controller.render.player.bio_android.third_person": "!v.is_first_person && !q.is_spectator && !v.map_face_icon && v.race == 'bio_android'"
				},
				// fusion
				{
					"controller.render.player.fusion_clothes.first_person": "v.is_first_person && !q.is_spectator && v.fusion_type != 'none'"
				},
				{
					"controller.render.player.fusion_clothes.third_person": "!v.is_first_person && !v.is_paperdoll && !q.is_spectator && !v.map_face_icon && v.fusion_type != 'none'"
				},
				// geral
				{
					"controller.render.player.aura": "v.aura_type == 'normal' && v.aura_enabled && !v.ultra_instinct"
				},
				{
					"controller.render.player.halo": "!v.is_first_person && v.is_death"
				},
				{
					"controller.render.player.ki_scythe": "v.ki_scythe_enabled && v.empty_hand"
				},
				{
					"controller.render.player.ki_scythe.outline": "v.ki_scythe_enabled && v.empty_hand"
				},
				{
					"controller.render.player.ki_manipulation": "v.is_loading_attack"
				},
				{
					"controller.render.player.ki_manipulation.outline": "v.is_loading_attack"
				},
				{
					"controller.render.player.brave_sword_sheath": "!v.is_first_person && v.brave_sword_sheath"
				},
				{
					"controller.render.player.power_pole_sheath": "!v.is_first_person && !v.brave_sword_sheath && v.power_pole_sheath"
				}
			],
			"enable_attachables": true
		}
	}
}