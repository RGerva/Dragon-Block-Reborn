                    "Array.colors": [
                        "Texture.default",
                        "Texture.brown_dinosaur",
                        "Texture.yellow_dinosaur"
                    ]
                }
            },
            "geometry": "Geometry.default",
            "materials": [ { "*": "Material.default" } ],
