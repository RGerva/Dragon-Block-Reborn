                        "Texture.frost_demon_skin_frost_demon_golden_form_1",
                        "Texture.frost_demon_skin_frost_demon_black_form_1"
                    ],
                    "Array.skin_2": [
                        "Texture.frost_demon_skin_2",
                        "Texture.frost_demon_skin_2",
                        "Texture.frost_demon_skin_third_form_2",
                        "Texture.frost_demon_skin_fourth_form_2",
                        "Texture.frost_demon_skin_fourth_form_2",
                        "Texture.frost_demon_skin_frost_demon_golden_form_2",
                        "Texture.frost_demon_skin_frost_demon_black_form_2"
