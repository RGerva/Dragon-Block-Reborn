                    ],
                    "Array.skin_4": [
                        "Texture.frost_demon_skin_4",
                        "Texture.frost_demon_skin_4",
                        "Texture.frost_demon_skin_third_form_4",
                        "Texture.frost_demon_skin_fourth_form_4",
