{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.player.frost_demon.eyes": {
            "arrays": {
                "textures": {
                    "Array.textures": [
                        "Texture.frost_demon_eyes",
                        "Texture.frost_demon_eyes",
                        "Texture.frost_demon_eyes",
                        "Texture.frost_demon_eyes_fourth_form",
                        "Texture.frost_demon_eyes_fourth_form",
