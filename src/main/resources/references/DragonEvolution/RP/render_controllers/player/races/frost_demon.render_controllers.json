                        "Texture.frost_demon_skin_frost_demon_golden_form_4",
                        "Texture.frost_demon_skin_frost_demon_black_form_4"
                    ]
                }
            },
            "geometry": "Geometry.frost_demon",
            "materials": [ { "*": "Material.default" } ],
            "textures": [ "v.skin == 0 ? Array.skin_1[v.transformation] : v.skin == 1 ? Array.skin_2[v.transformation] : v.skin == 2 ? Array.skin_3[v.transformation] : Array.skin_4[v.transformation]" ],
            "part_visibility": [
                { "*": true },
                { "headExtension": "v.transformation == 2" },
                { "leftEar": "!q.is_item_name_any('slot.armor.head', 'dbe:scouter')" },
                { "leftFrontHorn": "v.transformation < 3" },
                { "rightFrontHorn": "v.transformation < 3" },
                { "leftFrontHorn2": "v.transformation == 1 || v.transformation == 2" },
                { "rightFrontHorn2": "v.transformation == 1 || v.transformation == 2" },
                { "*leftBackHorn*": "v.transformation == 2" },
                { "*rightBackHorn*": "v.transformation == 2" },
                { "leftShoulderPad": "v.transformation == 2" },
                { "rightShoulderPad": "v.transformation == 2" },
                { "*tail*": "!v.tail_curled" }
            ],
            "overlay_color": {
