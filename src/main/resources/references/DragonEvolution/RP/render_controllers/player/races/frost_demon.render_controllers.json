                        "Texture.frost_demon_skin_frost_demon_golden_form_4",
                        "Texture.frost_demon_skin_frost_demon_black_form_4"
                    ]
                }
            },
            "geometry": "Geometry.frost_demon",
            "materials": [ { "*": "Material.default" } ],
            "textures": [ "v.skin == 0 ? Array.skin_1[v.transformation] : v.skin == 1 ? Array.skin_2[v.transformation] : v.skin == 2 ? Array.skin_3[v.transformation] : Array.skin_4[v.transformation]" ],
            "part_visibility": [
                { "*": false },
                { "rightArm": "v.rightarm_visible || q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'filled_map'" },
                { "rightSleeve": "v.rightarm_visible || q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'filled_map'" },
                { "leftArm": "v.leftarm_visible || (q.get_equipped_item_name(0, 1) == 'filled_map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'filled_map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))" },
                { "leftSleeve": "v.leftarm_visible || (q.get_equipped_item_name(0, 1) == 'filled_map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'filled_map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))" }
            ],
            "overlay_color": {
                "r": "v.kaioken_enabled ? 1 : 0",
                "g": 0,
                "b": 0,
                "a": "v.kaioken_enabled ? 0.2 : 0"
            }
        },
        "controller.render.player.frost_demon.third_person": {
