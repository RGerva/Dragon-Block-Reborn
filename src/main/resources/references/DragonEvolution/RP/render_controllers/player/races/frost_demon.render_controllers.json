                        "Texture.frost_demon_eyes_frost_demon_black_form"
                    ]
                }
            },
            "geometry": "Geometry.eyes",
            "materials": [
                { "*": "Material.default" },
                { "leftEye": "Material.entity_emissive" },
                { "rightEye": "Material.entity_emissive" }
            ],
            "textures": [ "Array.textures[v.transformation]" ],
            "overlay_color": {
                "r": "v.kaioken_enabled ? 1 : 0",
                "g": 0,
                "b": 0,
                "a": "v.kaioken_enabled ? 0.2 : 0"
            }
        },
        "controller.render.player.frost_demon.first_person": {
            "arrays": {
                "textures": {
                    "Array.skin_1": [
                        "Texture.frost_demon_skin_1",
