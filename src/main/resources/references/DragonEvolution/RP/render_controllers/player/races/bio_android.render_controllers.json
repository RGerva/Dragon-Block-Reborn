                        "Texture.bio_android_skin_perfect_form_2",
                        "Texture.bio_android_skin_perfect_form_2"
                    ],
                    "Array.skin_3": [
                        "Texture.bio_android_skin_imperfect_form_3",
                        "Texture.bio_android_skin_semi_perfect_form_3",
                        "Texture.bio_android_skin_perfect_form_3",
                        "Texture.bio_android_skin_perfect_form_3",
