                        "Texture.bio_android_skin_perfect_form_5",
                        "Texture.bio_android_skin_perfect_form_5"
                    ],
                    "Array.skin_6": [
                        "Texture.bio_android_skin_imperfect_form_6",
                        "Texture.bio_android_skin_semi_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6"
                    ]
                }
            },
            "geometry": "Geometry.bio_android",
            "materials": [ { "*": "Material.default" } ],
            "textures": [ "v.skin == 0 ? Array.skin_1[v.transformation] : v.skin == 1 ? Array.skin_2[v.transformation] : v.skin == 2 ? Array.skin_3[v.transformation] : v.skin == 3 ? Array.skin_4[v.transformation] : v.skin == 4 ? Array.skin_5[v.transformation] : Array.skin_6[v.transformation]" ],
            "part_visibility": [
                { "*": false },
                { "rightArm": "v.rightarm_visible || q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'map'" },
                { "rightForearm": "v.rightarm_visible || q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'map'" },
                { "leftArm": "v.leftarm_visible || (q.get_equipped_item_name(0, 1) == 'map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))" },
                { "leftForearm": "v.leftarm_visible || (q.get_equipped_item_name(0, 1) == 'map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))" }
            ],
            "overlay_color": {
                "r": "v.kaioken_enabled ? 1 : 0",
                "g": 0,
                "b": 0,
                "a": "v.kaioken_enabled ? 0.2 : 0"
            }
        },
        "controller.render.player.bio_android.third_person": {
            "arrays": {
                "textures": {
                    "Array.skin_1": [
                        "Texture.bio_android_skin_imperfect_form_1",
                        "Texture.bio_android_skin_semi_perfect_form_1",
                        "Texture.bio_android_skin_perfect_form_1",
                        "Texture.bio_android_skin_perfect_form_1",
                        "Texture.bio_android_skin_perfect_form_1"
                    ],
                    "Array.skin_2": [
                        "Texture.bio_android_skin_imperfect_form_2",
