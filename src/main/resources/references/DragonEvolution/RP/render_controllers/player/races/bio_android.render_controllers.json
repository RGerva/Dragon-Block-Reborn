{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.player.bio_android.eyes": {
            "arrays": {
                "textures": {
                    "Array.eyes": [
                        "Texture.bio_android_eyes_imperfect_form",
                        "Texture.bio_android_eyes_semi_perfect_form",
                        "Texture.bio_android_eyes_perfect_form",
                        "Texture.bio_android_eyes_perfect_form",
                        "Texture.bio_android_eyes_perfect_form"
                    ]
                }
            },
            "geometry": "Geometry.eyes",
            "materials": [
                { "*": "Material.default" },
                { "leftEye": "Material.entity_emissive" },
                { "rightEye": "Material.entity_emissive" }
            ],
            "textures": [ "Array.eyes[v.transformation]" ]
        },
        "controller.render.player.bio_android.first_person": {
            "arrays": {
                "textures": {
                    "Array.skin_1": [
                        "Texture.bio_android_skin_imperfect_form_1",
