                    ],
                    "Array.skin_4": [
                        "Texture.bio_android_skin_imperfect_form_4",
                        "Texture.bio_android_skin_semi_perfect_form_4",
                        "Texture.bio_android_skin_perfect_form_4",
                        "Texture.bio_android_skin_perfect_form_4",
                        "Texture.bio_android_skin_perfect_form_4"
                    ],
                    "Array.skin_5": [
                        "Texture.bio_android_skin_imperfect_form_5",
                        "Texture.bio_android_skin_semi_perfect_form_5",
                        "Texture.bio_android_skin_perfect_form_5",
                        "Texture.bio_android_skin_perfect_form_5",
                        "Texture.bio_android_skin_perfect_form_5"
                    ],
                    "Array.skin_6": [
                        "Texture.bio_android_skin_imperfect_form_6",
                        "Texture.bio_android_skin_semi_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6",
                        "Texture.bio_android_skin_perfect_form_6"
                    ]
                }
            },
            "geometry": "Geometry.bio_android",
            "materials": [ { "*": "Material.default" } ],
            "textures": [ "v.skin == 0 ? Array.skin_1[v.transformation] : v.skin == 1 ? Array.skin_2[v.transformation] : v.skin == 2 ? Array.skin_3[v.transformation] : v.skin == 3 ? Array.skin_4[v.transformation] : v.skin == 4 ? Array.skin_5[v.transformation] : Array.skin_6[v.transformation]" ],
            "part_visibility": [
                { "*": true },
                { "leftEar": "!q.is_item_name_any('slot.armor.head', 'dbe:scouter')" },
                { "horns": "!v.is_using_helmet && v.transformation == 0" },
                { "horns2": "!v.is_using_helmet && v.transformation != 0" },
                { "leftWing": "v.transformation != 1" },
                { "rightWing": "v.transformation != 1" },
                { "sting": "!v.tail_curled && v.transformation >= 2" },
                { "*tail*": "!v.tail_curled && v.transformation < 2" }
            ],
            "overlay_color": {
                "r": "v.kaioken_enabled ? 1 : 0",
                "g": 0,
                "b": 0,
                "a": "v.kaioken_enabled ? 0.2 : 0"
            }
        }
    }
}