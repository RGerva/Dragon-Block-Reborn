{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.potara_earring": {
            "geometry": "Geometry.default",
            "materials": [ { "*": "v.is_enchanted ? Material.enchanted : Material.default" } ],
            "textures": [ "Texture.default", "Texture.enchanted" ],
            "part_visibility": [
                { "*": true },
                { "leftEarring": "q.has_property('dbe:fusion_type') ? q.property('dbe:fusion_type') == 'potara' : false" }
            ]
        }
    }
}